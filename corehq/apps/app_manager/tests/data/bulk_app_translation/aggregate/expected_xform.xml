<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
    <h:head>
        <h:title>Untitled Form</h:title>
        <model>
            <instance>
                <data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="undefined" uiVersion="1" version="1" name="Untitled Form">
                    <with_markdown/>
                    <markdown_veto/>
                </data>
            </instance>
            <bind nodeset="/data/with_markdown" type="xsd:string"/>
            <bind nodeset="/data/markdown_veto" type="xsd:string"/>
            <itext>
                <translation lang="en" default="">
                    <text id="with_markdown-label">
                        <value>*With* Markdown</value><value form="markdown">*With* Markdown</value></text>
                    <text id="markdown_veto-label">
                        <value>*Without* Markdown</value></text>
                </translation>
                <translation lang="afr">
                    <text id="with_markdown-label">
                        <value>*Met* Markdown</value><value form="markdown">*Met* Markdown</value></text>
                    <text id="markdown_veto-label">
                        <value>*Sonder* Markdown</value></text>
                </translation>
                <translation lang="fra">
                    <text id="with_markdown-label">
                        <value>*Avec* le Markdown</value><value form="markdown">*Avec* le Markdown</value></text>
                    <text id="markdown_veto-label">
                        <value>*Sans* le Markdown</value></text>
                </translation>
            </itext>
        </model>
    </h:head>
    <h:body>
        <input ref="/data/with_markdown">
            <label ref="jr:itext('with_markdown-label')"/>
        </input>
        <input ref="/data/markdown_veto">
            <label ref="jr:itext('markdown_veto-label')"/>
        </input>
    </h:body>
</h:html>