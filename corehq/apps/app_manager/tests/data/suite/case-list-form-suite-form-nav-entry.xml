<partial>
    <entry>
        <command id="m1-f0">
          <text>
            <locale id="forms.m1f0"/>
          </text>
        </command>
        <instance id="casedb" src="jr://instance/casedb"/>
        <instance id="commcaresession" src="jr://instance/session"/>
        <session>
            <datum id="case_id" function="uuid()"/>
        </session>
        <stack>
            <create if="count(instance('commcaresession')/session/data/return_to) = 1 and count(instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]) &gt; 0">
                <command value="instance('commcaresession')/session/data/return_to"/>
                <datum id="case_id" value="instance('commcaresession')/session/data/case_id"/>
            </create>
            <create if="count(instance('commcaresession')/session/data/return_to) = 1 and count(instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]) = 0">
                <command value="instance('commcaresession')/session/data/return_to"/>
            </create>
            <create if="count(instance('commcaresession')/session/data/return_to) = 0">
              <command value="'m1'"/>
            </create>
        </stack>
    </entry>
</partial>