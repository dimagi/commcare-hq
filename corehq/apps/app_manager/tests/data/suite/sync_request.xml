<partial>
  <sync-request>
    <post url="https://www.example.com/a/test_domain/phone/claim-case/">
      <data key="case_id"
            ref="instance('results')/results/case[@case_type='case'][@status='open'][0]/@case_id"/>
      <data key="case_type"
            ref="instance('results')/results/case[@case_type='case'][@status='open'][0]/@case_type"/>
      <data key="case_name"
            ref="instance('results')/results/case[@case_type='case'][@status='open'][0]/name"/>
    </post>
    <command id="search_command.m0">
      <display>
        <text>
          <locale id="case_search.m0"/>
        </text>
      </display>
    </command>
    <session>
      <datum id="case_id"
             nodeset="instance('results')/results/case[@case_type='case'][@status='open']"
             value="./@case_id"
             detail-confirm="m0_case_long"
             detail-select="m0_case_short"/>
      <query url="https://www.example.com/a/test_domain/phone/search/"
             storage-instance="results">
        <prompt key="name">
          <display>
            <text>
              <locale id="search_property.m0.name"/>
            </text>
          </display>
        </prompt>
        <prompt key="dob">
          <display>
            <text>
              <locale id="search_property.m0.dob"/>
            </text>
          </display>
        </prompt>
        <data key="case_type" ref="'case'"/>
      </query>
    </session>
    <stack>
      <push>
        <command value="'m0-f0'"/>
        <datum id="calculated_data" value="'claimed'"/>
      </push>
    </stack>
  </sync-request>
</partial>
