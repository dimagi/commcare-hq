    </style>
    <header>
      <text/>
    </header>
    <template form="{top_left_image[format]}">
      <text>
        <xpath function="{top_left_image[xpath_function]}">
          {top_left_image[variables]}
        </xpath>
      </text>
    </template>
    {top_left_image[endpoint_action]}
