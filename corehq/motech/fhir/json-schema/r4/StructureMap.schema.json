            },
            "mode": {
              "description": "How the referenced structure is used in this mapping.",
              "enum": [
                "source",
                "queried",
                "target",
                "produced"
              ],
              "type": "string"
            },
            "_mode": {
              "description": "Extensions for mode",
              "$ref": "Element.schema.json#/definitions/Element"
