msgstr "No, Solucione el Problema"

#: corehq/messaging/smsbackends/telerivet/templates/telerivet/partials/step2.html:74
msgid "Yes, Continue"
msgstr "Sí, Continúe"

#: corehq/messaging/smsbackends/telerivet/templates/telerivet/partials/step2.html:79
msgid "Troubleshoot Outbound SMS"
msgstr "Solucione el Problema de SMS Salientes"

#: corehq/messaging/smsbackends/telerivet/templates/telerivet/partials/step2.html:87
msgid ""
"\n"
"                    Double-check your configuration in step 2.\n"
"                    "
msgstr ""

#: corehq/messaging/smsbackends/telerivet/templates/telerivet/partials/step2.html:92
msgid ""
"\n"
"                    Double-check the phone number of your test phone number "
"entered in step 3 and\n"
"                    ensure that it is in international format, starting with "
"the country code.\n"
"                    "
msgstr ""

