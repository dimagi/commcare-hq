#: corehq/apps/hqwebapp/templates/home.html:37
msgid ""
"\n"
"    <div class=\"three-columns\">\n"
"        <div class=\"col-1\">\n"
"            <p class=\"subheading\" >What is CommCare?</p>\n"
"            <ul class=\"summary\">\n"
"                <li>A free and open-source software application with mobile and server-side components</li>\n"
"                <li>A job aid for community health workers (CHWs)</li>\n"
"                <li>A tool for supervision and evidence-based change</li>\n"
"                <li>A way to capture data in an electronic repository that otherwise sits in thousands of paper notebooks</li>\n"
"            </ul>\n"
"        </div>\n"
"        <div class = \"col-2\">\n"
"            <p class=\"subheading\" >Why CommCare?</p>\n"
"            <ul class=\"summary\">\n"
"                <li>CHWs have demonstrated a potential to substantially improve health outcomes in areas of extreme poverty</li>\n"
"                <li>CHWs tend to have little training and operate independently</li>\n"
"                <li>CommCare provides phone-based tools that help support and manage large numbers of CHWs</li>\n"
"                <li>CommCare can be an educational tool and portable reference manual allowing CHWs to deliver better care</li>\n"
"            </ul>\n"
"        </div>\n"
"        <div class = \"col-3\">\n"
"            <p class=\"subheading\" >Highlights</p>\n"
"            <ul class=\"summary\">\n"
"            <li>Runs on a variety of cell phones and devices, from Nokia to Android</li>\n"
"            <li>Provides portable case management, tracking patients and outcomes over time</li>\n"
"            <li>Supports image and audio prompts</li>\n"
"            <li>Handles multiple languages and scripts including English, Spanish, Swahili, Hindi, and Dari</li>\n"
"            <li>Is fully customizable, without paying a technical team</li>\n"
"        </ul>\n"
"        </div>\n"
"    </div>\n"
"    "
msgstr "\n    <div class=\"three-columns\">\n        <div class=\"col-1\">\n            <p class=\"subheading\" >What is CommCare?</p>\n            <ul class=\"summary\">\n                <li>A free and open-source software application with mobile and server-side components</li>\n                <li>A job aid for community health workers (CHWs)</li>\n                <li>A tool for supervision and evidence-based change</li>\n                <li>A way to capture data in an electronic repository that otherwise sits in thousands of paper notebooks</li>\n            </ul>\n        </div>\n        <div class = \"col-2\">\n            <p class=\"subheading\" >Why CommCare?</p>\n            <ul class=\"summary\">\n                <li>CHWs have demonstrated a potential to substantially improve health outcomes in areas of extreme poverty</li>\n                <li>CHWs tend to have little training and operate independently</li>\n                <li>CommCare provides phone-based tools that help support and manage large numbers of CHWs</li>\n                <li>CommCare can be an educational tool and portable reference manual allowing CHWs to deliver better care</li>\n            </ul>\n        </div>\n        <div class = \"col-3\">\n            <p class=\"subheading\" >Highlights</p>\n            <ul class=\"summary\">\n            <li>Runs on a variety of cell phones and devices, from Nokia to Android</li>\n            <li>Provides portable case management, tracking patients and outcomes over time</li>\n            <li>Supports image and audio prompts</li>\n            <li>Handles multiple languages and scripts including English, Spanish, Swahili, Hindi, and Dari</li>\n            <li>Is fully customizable, without paying a technical team</li>\n        </ul>\n        </div>\n    </div>\n    "

