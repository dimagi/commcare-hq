      {
        "field": "count",
        "type": "field",
        "display": "Count",
        "aggregation": "sum"
      }
    ],
    "sort_expression": [],
    "configured_charts": [
      {
        "value_column": "count",
        "y_axis_columns": [
          {
            "display": "Number of Issues",
            "column_id": "count"
          }
        ],
        "type": "multibar",
        "x_axis_column": "problem_area",
        "title": "Problem Area"
      }
