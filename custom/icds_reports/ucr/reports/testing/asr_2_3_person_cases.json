        "display": "M_minority_count",
        "column_id": "M_minority_count",
        "type": "sum_when",
        "aggregation": "sum",
        "calculate_total": true,
        "whens": {
          "closed_on IS NULL AND sex IN ('M', '0') and hh_minority = 1": 1
        }
      },
      {
        "display": "M_disabled_count",
        "column_id": "M_disabled_count",
        "type": "sum_when",
        "aggregation": "sum",
        "calculate_total": true,
