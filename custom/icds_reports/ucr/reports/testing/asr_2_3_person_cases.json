        "aggregation": "sum",
        "calculate_total": true,
        "whens": {
          "closed_on IS NULL AND sex IN ('M', 'O') and hh_caste = 'sc'": 1
        }
      },
      {
        "display": "M_other_count",
        "column_id": "M_other_count",
        "type": "sum_when",
        "aggregation": "sum",
        "calculate_total": true,
        "whens": {
          "closed_on IS NULL AND sex IN ('M', 'O') and hh_caste NOT IN ('st', 'sc')": 1
        }
      },
