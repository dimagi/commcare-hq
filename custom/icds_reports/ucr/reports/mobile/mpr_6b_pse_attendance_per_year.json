  "report_id": "static-mpr_6b_pse_attendance_per_year",
  "data_source_table": "static-child_cases_monthly_v2",
  "config": {
    "title": "MPR 6b - PSE Attendance by year (Static)",
    "description": "",
    "visible": false,
    "aggregation_columns": [
      "owner_id",
      "month",
      "age_group"
    ],
    "filters": [
      {
        "slug": "age_in_months_in_bounds",
        "type": "pre",
        "field": "age_in_months",
        "pre_operator": "between",
        "pre_value": [36, 72],
        "datatype": "integer"
      },
      {
        "display": "Month for Data",
        "slug": "month",
        "type": "date",
        "field": "month",
        "datatype": "date"
      },
      {
        "display": "Filter by AWW",
        "slug": "awc_id",
        "type": "dynamic_choice_list",
        "field": "awc_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by Supervisor",
        "slug": "supervisor_id",
        "type": "dynamic_choice_list",
        "field": "supervisor_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by Block",
        "slug": "block_id",
        "type": "dynamic_choice_list",
        "field": "block_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by District",
        "slug": "district_id",
        "type": "dynamic_choice_list",
        "field": "district_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by State",
        "slug": "state_id",
        "type": "dynamic_choice_list",
        "field": "state_id",
        "choice_provider": {
          "type": "location"
        }
      }
    ],
    "columns": [
      {
        "display": {
          "en": "Owner",
          "hin": "Owner"
        },
        "column_id": "owner_id",
        "type": "field",
        "field": "awc_id",
        "aggregation": "simple",
        "transform": {
          "type": "custom",
          "custom_type": "owner_display"
        }
      },
      {
        "display": "Month",
        "column_id": "month",
        "type": "aggregate_date",
        "field": "month",
        "format": "%Y-%m"
      },
      {
        "display": "Age Group",
        "column_id": "age_group",
        "type": "conditional_aggregation",
        "whens": {
            "age_in_months BETWEEN 36 AND 47": "36_to_47",
            "age_in_months BETWEEN 48 AND 59": "48_to_59",
            "age_in_months BETWEEN 60 AND 72": "60_to_72"
        }
      },
      {
        "display": "pse_daily_attendance_male",
        "column_id": "pse_daily_attendance_male",
        "type": "field",
        "field": "pse_daily_attendance_male",
        "aggregation": "sum",
        "calculate_total": true
      },
      {
        "display": "pse_daily_attendance_female",
        "column_id": "pse_daily_attendance_female",
        "type": "field",
        "field": "pse_daily_attendance_female",
        "aggregation": "sum",
        "calculate_total": true
      }
    ],
    "sort_expression": [],
    "configured_charts": []
  }
