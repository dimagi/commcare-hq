  "config": {
    "table_id": "static-home_visit_forms",
    "display_name": "Forms - Home Visit (Static)",
    "referenced_doc_type": "XFormInstance",
    "description": "The number of AWC Management forms Supervisors in an Area have filled in the last 30 days",
    "base_item_expression": {},
    "configured_filter": {
      "operator": "in",
      "type": "boolean_expression",
      "expression": {
        "datatype": null,
        "type": "property_name",
        "property_name": "xmlns"
      },
      "property_value": [
        "http://openrosa.org/formdesigner/2864010F-B1B1-4711-8C59-D5B2B81D65DB",
        "http://openrosa.org/formdesigner/D4A7ABD2-A7B8-431B-A88B-38245173B0AE",
        "http://openrosa.org/formdesigner/376FA2E1-6FD1-4C9E-ACB4-E046038CD5E2",
        "http://openrosa.org/formdesigner/89097FB1-6C08-48BA-95B2-67BCF0C5091D",
        "http://openrosa.org/formdesigner/792DAF2B-E117-424A-A673-34E1513ABD88"
      ]
    },
    "validations": [
      {
        "name": "submitted_by_aww",
        "error_message": "Form not submitted by AWW",
        "expression": {
          "type": "boolean_expression",
          "operator": "eq",
          "expression": {
            "location_id_expression": {
              "type": "named",
              "name": "user_location_id"
            },
            "type": "location_type_name"
          },
          "property_value": "awc"
        }
      }
    ],
    "configured_indicators": [
      {
        "display_name": "AWC ID",
        "datatype": "string",
        "type": "expression",
        "transform": {},
        "is_nullable": true,
        "is_primary_key": false,
        "column_id": "awc_id",
        "expression": {
          "type": "named",
          "name": "user_location_id"
        }
      },
      {
        "display_name": "Supervisor ID",
        "datatype": "string",
        "type": "expression",
        "transform": {},
        "is_nullable": false,
        "is_primary_key": true,
        "column_id": "supervisor_id",
        "expression": {
          "location_id_expression": {
            "type": "named",
            "name": "user_location_id"
          },
          "type": "location_parent_id"
        }
      },
      {
        "display_name": "Submission Date",
        "transform": {},
        "datatype": "date",
        "type": "expression",
        "is_primary_key": false,
        "is_nullable": true,
        "column_id": "submitted_on",
        "expression": {
          "datatype": "date",
          "type": "property_path",
          "property_path": [
            "form",
            "meta",
            "timeEnd"
          ]
        }
      },
      {
        "filter": {
          "type": "and",
          "filters": [
            {
              "operator": "eq",
              "type": "boolean_expression",
              "expression": {
                "datatype": null,
                "type": "property_path",
                "property_path": [
                  "form",
                  "unscheduled_visit"
                ]
              },
              "property_value": "0"
            },
            {
              "operator": "lte",
              "type": "boolean_expression",
              "expression": {
                "datatype": null,
                "type": "property_path",
                "property_path": [
                  "form",
                  "days_visit_late"
                ]
              },
              "property_value": "3"
            }
          ]
        },
        "display_name": null,
        "type": "boolean",
        "column_id": "visit_on_time"
      },
      {
        "type": "count",
        "display_name": "Count",
        "column_id": "count"
      },
      {
        "display_name": "xmlns",
        "transform": {},
        "datatype": "string",
        "type": "expression",
        "is_primary_key": false,
        "is_nullable": true,
        "column_id": "xmlns",
        "expression": {
          "datatype": null,
          "type": "property_name",
          "property_name": "xmlns"
        }
      }
    ],
    "named_expressions": {
      "user_location_id": {
        "type": "icds_user_location",
        "user_id_expression": {
          "type": "root_doc",
          "expression": {
            "type": "property_path",
            "property_path": [
              "form",
              "meta",
              "userID"
            ]
          }
        }
      }
    },
    "named_filters": {},
    "engine_id": "icds-ucr-non-dashboard",
    "sql_settings": {
      "citus_config": {
        "distribution_type": "hash",
        "distribution_column": "supervisor_id"
      },
      "primary_key": ["supervisor_id","doc_id"]
    },
    "sql_column_indexes": [
      {"column_ids": ["supervisor_id", "submitted_on"]}
    ],
    "disable_destructive_rebuild": true
  }
