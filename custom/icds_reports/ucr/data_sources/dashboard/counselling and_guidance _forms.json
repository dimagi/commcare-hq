          "location_type": "block",
          "location_property": "_id",
          "type": "ancestor_location"
        },
        "is_primary_key": false,
        "is_nullable": true,
        "column_id": "block_id",
        "type": "expression"
      },
      {
        "display_name": "District ID",
        "transform": {},
        "datatype": "string",
        "expression": {
          "location_id": {
            "type": "named",
            "name": "user_location_id"
          },
          "location_type": "district",
          "location_property": "_id",
          "type": "ancestor_location"
        },
        "is_primary_key": false,
        "is_nullable": true,
        "column_id": "district_id",
        "type": "expression"
      },
      {
        "display_name": "State ID",
        "transform": {},
        "datatype": "string",
        "expression": {
          "location_id": {
            "type": "named",
            "name": "user_location_id"
          },
          "location_type": "state",
          "location_property": "_id",
          "type": "ancestor_location"
        },
        "is_primary_key": false,
        "is_nullable": true,
        "column_id": "state_id",
        "type": "expression"
      },
      {
        "column_id": "received_on",
        "datatype": "datetime",
        "type": "expression",
        "expression": {
          "type": "root_doc",
          "expression": {
            "type": "property_name",
            "datatype": "datetime",
            "property_name": "received_on"
          }
        }
      },
      {
        "column_id": "child_health_case_id",
