      },
      "property_value": "http://openrosa.org/formdesigner/A2604F2C-F52D-4B67-889F-C2F9B44698DD"
    },
    "validations": [
      {
        "name": "submitted_by_aww",
        "error_message": "Form not submitted by AWW",
        "expression": {
          "type": "boolean_expression",
          "operator": "eq",
          "expression": {
            "location_id_expression": {
              "type": "named",
              "name": "user_location_id"
            },
            "type": "location_type_name"
          },
          "property_value": "awc"
        }
      }
    ],
