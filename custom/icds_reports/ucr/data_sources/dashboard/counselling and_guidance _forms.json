        }
      },
      {
        "display_name": "AWC ID",
        "transform": {},
        "datatype": "string",
        "expression": {
          "type": "named",
          "name": "user_location_id"
        },
        "is_primary_key": false,
        "is_nullable": true,
        "column_id": "awc_id",
        "type": "expression"
      },
      {
        "display_name": "Supervisor ID",
        "transform": {},
        "datatype": "string",
        "expression": {
          "location_id": {
            "type": "named",
            "name": "user_location_id"
          },
          "location_type": "supervisor",
          "location_property": "_id",
          "type": "ancestor_location"
