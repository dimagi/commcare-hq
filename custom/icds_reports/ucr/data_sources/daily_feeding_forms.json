      "attended_children": {
        "comment": "This expression has logic to deal with a bug in the app.  The app version before 10324 counts an extra child if active is blank and inactive is not since it concats an extra space",
        "type": "conditional",
        "test": {
          "type": "and",
          "filters": [
            {
              "type": "boolean_expression",
              "operator": "lt",
              "property_value": 10324,
              "expression": {
                "type": "icds_get_app_version",
                "datatype": "integer",
                "app_version_string": {
                  "type": "property_path",
                  "property_path": [
                    "form",
                    "meta",
                    "appVersion",
                    "#text"
                  ]
                }
              }
            },
            {
              "type": "boolean_expression",
              "operator": "in",
              "property_value": ["", null],
              "expression": {
                "type": "property_path",
                "property_path": [
                  "form",
                  "child_attendance_ql",
                  "attendance_active"
                ]
              }
            },
            {
              "type": "not",
              "filter": {
                "type": "boolean_expression",
                "operator": "in",
                "property_value": ["", null],
                "expression": {
                  "type": "property_path",
                  "property_path": [
                    "form",
                    "child_attendance_ql",
                    "attendance_inactive"
                  ]
                }
              }
            }
          ]
        },
        "expression_if_true": {
          "type": "evaluator",
          "datatype": "integer",
          "statement": "num_attended_children - 1",
          "context_variables": {
            "num_attended_children": {
              "datatype": "integer",
              "type": "property_path",
              "property_path": [
                "form",
                "num_attended_children"
              ]
            }
          }
        },
        "expression_if_false": {
          "datatype": "integer",
          "type": "property_path",
          "property_path": [
            "form",
            "num_attended_children"
          ]
        }
      },
